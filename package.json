{
  "name": "modsen-test",
  "version": "1.0.0",
  "description": "meetup",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "rimraf dist && tsc",
    "start": "node .",
    "server:watch": "cross-env APP_NODE_ENV=development ts-node-dev --respawn --transpile-only src/server.ts",
    "dev": "docker-compose up",
    "lint": "eslint --cache --ext .ts .",
    "lint:fix": "eslint --cache --ext .ts . --fix",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:create": "npm run typeorm -- migration:create src/database/migrations/%npm_config_name%",
    "migration:generate": "npm run typeorm -- -d src/providers/db/postgres.ts migration:generate src/database/migrations/%npm_config_name%",
    "migration:run": "npm run typeorm migration:run -- -d src/providers/db/postgres.ts",
    "migration:down": "npm run typeorm -- -d src/providers/db/postgres.ts migration:revert",
    "db:seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -d src/providers/db/postgres.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "log4js": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "typeorm-extension": "^2.8.1",
    "@faker-js/faker": "^8.0.2",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "@types/passport-jwt": "^3.0.8",
    "@types/pg": "^8.10.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui": "^3.52.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./src/config/commitizen.ts"
    }
  }
}
